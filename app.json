{
  "expo": {
    "name": "Clubhouse Golf",
    "slug": "GolfApp",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/AppIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/LogoFull.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "clubhousegolf",
